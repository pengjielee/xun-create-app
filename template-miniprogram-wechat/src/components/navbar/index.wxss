.p-navbar {
  width: 100%;
  position: sticky;
  top: 0;
  left: 0;
  z-index: 10;
  /*background: linear-gradient(-90deg, #ece2d5 0%, #f7f4ef 100%);*/
}

.p-navbar_title {
  width: 100%;
  box-sizing: border-box;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 10;
  color: #333;
  font-size: 16px;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
